{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": 0,
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "jsx-a11y/no-autofocus": "off",
      "no-underscore-dangle": 0
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": ["react"],
    "env": {
      "browser": true,
      "commonjs": true,
      "jest": true
    }
  }
  